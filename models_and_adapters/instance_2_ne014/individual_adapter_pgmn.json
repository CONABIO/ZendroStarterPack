{
  "model": "Individual",
  "storageType": "cenzontle-web-service-adapter",
  "adapterType": "remote",
  "adapterName": "INDIVIDUAL_PGMN",
  "regex": "pgmn",
  "url": "http://localhost:4040/graphql",
  "attributes": {
    "name": "String",
    "origin": "String",
    "description": "String",
    "accessionId": "Int",
    "genotypeId": "Int",
    "field_unit_id": "Int"
  },

  "associations":{
    "accession":{
      "type": "to_one",
      "target": "Accession",
      "targetKey": "accessionId",
      "keyIn" : "Individual",
      "targetStorageType": "distributed-data-model",
      "label": "accession_id",
      "sublabel": "institution_deposited"
    },

    "measurements": {
      "type": "to_many",
      "target": "Measurement",
      "targetKey": "individual_id",
      "keyIn" : "Measurement",
      "targetStorageType": "distributed-data-model",
      "label": "name"
    }
  },
  "internalId" : "name"
}

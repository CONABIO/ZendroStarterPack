{
  "model": "Location",
  "storageType": "distributed-data-model",
  "registry": ["LOCATION_YOLANDAPROJECT", "LOCATION_PGMN"],
  "attributes" : {
    "country": "String",
    "state": "String",
    "municipality": "String",
    "locality": "String",
    "latitude": "Float",
    "longitude": "Float",
    "altitude": "Float"

{
  "model" : "SequencingExperiment",
  "storageType": "sql",
  "attributes":{
      "name": "String",
      "start_date": "String",
      "end_date": "String",
      "description": "String",
      "protocol": "String",
      "platform": "String",
      "data_type": "String",
      "library_type": "String",
      "library_preparation": "String",
      "aimed_coverage": "Int",
      "resulting_coverage": "Int",
      "insert_size": "Int",
      "aimed_real_length": "Int",
      "genome_complexity_reduction": "String",
      "enzyme": "String",
      "contaminations": "String",
      "sequencing_place": "String",
      "flowcell": "String",
      "sequencing_lane": "String",
      "protocol_reference_link": "String"
    },
   "associations": {
      "result_libraries" : {
      "type": "hasMany",
      "target": "NucAcidLibraryResult",
      "targetKey": "sequencing_experiment_id",
      "targetStorageType": "sql",
      "label": "lab_code" ,
      "sublabel": "file_name"
    },

      "samples": {
        "type": "belongsToMany",
        "target": "Sample",
        "targetKey": "sample_id",
        "sourceKey": "experiment_id",
        "keysIn": "sample_to_experiment",
        "targetStorageType": "sql",
        "label": "name",
        "sublabel": "tissue"
      },

      "plates": {
        "type": "hasMany",
        "target": "Plate",
        "targetKey": "experiment_id",
        "targetStorageType": "sql",
        "label": "plate_id" ,
        "sublabel": "barcode"
      }

   }
 }

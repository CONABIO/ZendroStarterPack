{
    "model": "institution",
    "storageType": "sql",
    "attributes": {
        "name": "String",
        "address": "String",
        "phone_number": "Int",
        "cumulus_ids": "[Int]",
        "created_at": "DateTime"
    },
    "associations": {
        "associated_cumulus": {
            "type": "many_to_many",
            "implementation": "foreignkeys",
            "reverseAssociation": "associated_institutions",
            "target": "cumulus",
            "targetKey": "institutions_ids",
            "sourceKey": "cumulus_ids",
            "keysIn": "institution",
            "targetStorageType": "sql"
        },
        "users": {
            "type": "one_to_many",
            "implementation": "foreignkeys",
            "reverseAssociation": "institutions",
            "target": "user",
            "targetKey": "institution_id",
            "keysIn": "user",
            "targetStorageType": "sql"
        }
    }
}
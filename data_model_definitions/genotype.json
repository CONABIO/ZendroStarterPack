{
  "model" : "Genotype",
  "storageType": "sql",
  "attributes":{
      "name": "String",
      "description": "String",
      "origin": "String",
      "description": "String"
    },
   "associations": {
     "mother" : {
        "type": "belongsTo",
        "target": "Individual",
        "targetKey": "mother_id",
        "targetStorageType": "sql",
        "label": "name"
      },

      "father" : {
        "type": "belongsTo",
        "target": "Individual",
        "targetKey": "father_id",
        "targetStorageType": "sql",
        "label": "name"
      },

      "pools" : {
        "type": "hasMany",
        "target": "BreedingPool",
        "targetKey": "genotype_id",
        "targetStorageType": "sql",
        "label": "name"
      },

      "individuals" : {
        "type": "hasMany",
        "target": "Individual",

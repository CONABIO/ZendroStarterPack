{
  "model" : "NucAcidLibraryResult",
  "storageType": "sql",
  "attributes":{
      "lab_code": "String",
      "file_name": "String",
      "file_uri": "String",
      "type": "String",
      "insert_size": "Int",
      "technical_replicate": "Int",
      "trimmed": "Boolean",
      "trimming_method": "String",
      "demultiplexed": "String",
      "demultiplexing": "String"
    },
   "associations": {
     "sequencing_experiment" : {
      "type": "belongsTo",
      "target": "SequencingExperiment",
      "targetKey": "sequencing_experiment_id",
      "targetStorageType": "sql",
      "label": "name" ,
      "sublabel": "sequencing_place"
    },

      "samples" : {
        "type": "belongsToMany",
        "target": "Sample",
        "targetKey": "sample_id",
        "sourceKey": "library_result_id",
        "keysIn": "sample_to_library_results",
        "targetStorageType": "sql",
        "label": "name",
        "sublabel": "tissue"
      },

      "genotyping_data": {
        "type": "belongsToMany",
        "target": "GenotypingData",
        "targetKey": "genotype_data_id",
        "sourceKey": "library_result_id",
        "keysIn": "genotyping_to_library_results",
        "targetStorageType": "sql",
        "label": "file_name"
      }
   }
 }
